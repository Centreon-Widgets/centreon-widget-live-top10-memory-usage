<configs>
        <title>Top10_memory</title>
        <author>Centreon</author>
        <email>contact@centreon.com</email>
        <website>http://www.centreon.com</website>
        <description>Widget for top 10 hosts in function % memory</description>
        <version>1.0</version>
        <keywords>centreon, widget, memory</keywords>
        <screenshot></screenshot>
        <thumbnail>./widgets/Top10_memory/resources/logo1.png</thumbnail>
        <url>./widgets/Top10_memory/index.php</url>
        <autoRefresh>10</autoRefresh>
	<preferences>
	  <preference label="Titre" name="title" defaultValue="TOP 10 - Most memory consuming resources" type="text"/>
         <preference label="Name Host Group" name="host_group" defaultValue="" type="hostgroup" header="Resource"/>
	 <preference label="Metric Name" name="metric_name" defaultValue="used" type="text"/>
         <preference label="Service Description" name="service_description" defaultValue="memory" type="text"/>
	  <preference label="Nombre de ligne" name="nb_lin" defaultValue="10" type="text"/>
	</preferences>
</configs>
